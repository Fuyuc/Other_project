@import "../../template/template.wxss";
/* 查询信息列表上拉记载页面模板样式 */
.list-Style1{
  visibility: hidden;
  margin-top: -7vh;
  width: 100vw;
  background-color: white;  
  border-top: #f5f5f5 1vh solid;
}
.list-Style2{
  visibility: visible;
  margin-top: -7vh;
  width: 100vw;
  background-color: white;  
  border-top: #f5f5f5 1vh solid;
}
.top{
  padding: 2vh 2vw;
  width: 100%;
  height: 14vh;
  display: flex;
     background-color: #fff ;   
}
.list-Photo{
  float: left;
  width: 19vw;
  height: 13vh;
  margin-left: 2vw;
  font-size: 72rpx;
  font-weight: bold;
  text-align: center;
  line-height: 13vh;
  border: #D7D6DC 1px solid;
  box-shadow: 10rpx 10rpx 10rpx #888888;
}
.list-Info{
  float: left;
  margin-left: 4vw;
  width: 76%;
  height: 16vh; 
  /* background-color: beige;  */
}
.list-infoTop{
  width: 100%;
  height: 8vh;
  line-height: 4vh;
  /* background-color: red;  */
}
.list-topName{
  float: left;
  font-weight: bold;
  line-height: 100%;
  color: #459df5;
}
.list-tree{
  width: 20vw;
  height: 5vh;
  line-height: 5vh;
  font-size: 28rpx;
  border-radius: 5%;
  border: red 1px solid;
  background-color: red;
  text-align: center;
  color: white;
  box-shadow: 5rpx 5rpx 5rpx #888888;
}

.class_item{
  border-bottom: solid #f5f5f5 1px;
  padding: 36rpx;
}
.class_item_end{
  text-align: center;
}





/*第三部分*/
.doctor-bgcolor-white{
  border-top: 1vh #EDECEB solid;
  width: 100%;
  background-color: white;
  height: 81vh;
}
.doctor-third{
  margin: 20rpx 30rpx 0rpx 30rpx; 
  text-align: left;
}
.doctor-tips{
  border-left: 10rpx #BB1C21 solid;
  font-size: 34rpx;
  padding-left: 20rpx;
  color: black;
}
.doctor-info{
  padding: 12rpx 0;
  font-size: 26rpx;
  line-height: 52rpx;
  text-indent: 56rpx;
}

















