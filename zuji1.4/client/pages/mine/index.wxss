page {
  width: 100vw;
  height: 100%;
  overflow: hidden;
}

navigator {
  height: 100%;
  width: 100%;
  display: block;
}

.flex {
  display: flex;
}

.container {
  width: 100%;
  height: 100%;
}

.partition {
  width: 100%;
  margin-top: 25rpx;
  margin-bottom: 5rpx;
  background-color: white;
  border-top: 3.5rpx solid #f2f2f2;
  border-bottom: 3.5rpx solid #f2f2f2;
}

.setion {
  padding: 20rpx 20rpx 20rpx 10rpx;
  margin-left: 30rpx;
  background-color: #fff;
  color: #787878;
  border-bottom: 1.5rpx solid #f2f2f2;
  align-items: center;
}

.setion-middle {
  flex: 1;
  margin-left: 40rpx;
  padding-top: 4rpx;
  overflow: hidden;
  justify-content: flex-start;
  align-items: middle;
  font-size: 28rpx;
  color: #353535;
}

.middle-up {
  align-items: flex-end;
  margin-bottom: 10rpx;
}

.middle-up .name {
  font-size: 35rpx;
  color: #353535;
}

.middle-up .tel {
  font-size: 30rpx;
  color: #353535;
}

.middle-down {
  align-items: flex-end;
}

.middle-down .introduce {
  font-size: 25rpx;
  color: #353535;
}

.image{
  width: 45rpx; 
  height: 45rpx;
  align-items:center;
}

.button {
  width: 755rpx;
  height: 110rpx;
  background-color: white;
  padding: 0rpx;
}

.button::after {
  border: none;
}

.setion_service {
  margin-left: 40rpx;
  justify-content: flex-start;
  align-items: middle;
  font-size: 28rpx;
  color: #353535;
}
